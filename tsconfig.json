{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": ["es6", "dom"],
    "baseUrl": ".",
    "paths": {
      "tmw-main/*": ["react/app-main/*"],
      "tmw-admin/*": ["react/app-admin/*"],
      "tmw-common/*": ["react/common/*"]
    },
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true
  },
  "include": ["react/**/*"]
}
