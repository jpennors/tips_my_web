{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": ["es6", "dom"],
    "baseUrl": ".",
    "paths": {
      "tmw/*": ["resources/assets/react/*"]
    },
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true
  },
  "include": ["resources/assets/react/**/*"]
}
