{
  "compilerOptions": {
    "module": "esnext", // Target module resolution
    "moduleResolution": "Node", // Current module resolution
    "target": "es2020", // Target ECMA version
    "lib": [
      "es2020",
      "dom"
    ], // Current ECMA version
    "baseUrl": ".",
      "paths": {
        "tmw-main/*": ["app-main/*"],
        "tmw-admin/*": ["app-admin/*"],
        "tmw-common/*": ["common/*"]
      },
    "sourceMap": true,
    "jsx": "preserve",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "pages/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
