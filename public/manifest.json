      "type": "image/png"
    },
    {
      "src": "/images/logo.png",
      "sizes": "150x150",
      "type": "image/png"
    },
    {
      "src": "/images/logo.png",
      "sizes": "152x152",
      "type": "image/png"
    },
    {
      "src": "/images/logo.png",
      "sizes": "180x180",
      "type": "image/png"
    },
    {
      "src": "/images/logo.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/images/logo.png",
      "sizes": "512x512",
      "type": "image/png"
    }
